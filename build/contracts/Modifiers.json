  "source": "// SPDX-License-Identifier: GPL-3.0\n\npragma solidity <0.9.0;\n\ncontract Modifiers {\n\n    address manager;\n    uint public state;\n\n\n    modifier onlyOwner() {\n        require(msg.sender == manager);\n        _;\n    }\n\n    modifier redOnly() {\n        require(state == 0, \"Must be in RED state\");\n        _;\n    }\n\n    modifier greenOnly() {\n        require(state == 2, \"Must be in GREEN state\");\n        _;\n    }\n\n    modifier notRed() {\n        require(state != 0, \"Must not be in RED state\");\n        _;\n    }\n\n\n}",
